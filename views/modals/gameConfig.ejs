<div>
  <label for="listChars"> Single select: </label><br>
  <select name="listChars" ng-model="selectedChar">
    <option ng-repeat="option in listChars track by $index" value="{{$index}}">{{option.name}}</option>
  </select>
  <input min="0" max="10" ng-if="gameGuid === undefined" type="number" name="maxPlayers" ng-model="maxPlayers">

  <button ng-click="ok()">Create</button>
</div>
